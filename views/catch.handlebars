{{> header}}

<div class="container-fluid text-center">
  <h1>Catch</h1>
</div>

<div class="container-fluid col-md-offset-2">
  <form class="form-inline">
      <div class="row">
        <div class="form-group ">
          <input type="date" class="form-control" id="addCatchTime" placeholder="Catch Time">
        </div>
        <div class="form-group ">
          <input type="number" class="form-control" id="addPlayerID" placeholder="PlayerID">
        </div>
        <div class="form-group ">
          <input type="number" class="form-control" id="addLocationID" placeholder="LocationID">
        </div>
        <div class="form-group ">
          <input type="number" class="form-control" id="addPokemonID" placeholder="PokemonID">
        </div>
        <div class="form-group edit-buttons">
          <button id="addRow" class="btn btn-default" role="button" type="button" name="button">+</button>
          <button id="reset" class="btn btn-danger" type="button" name="button"><span class="" aria-hidden="true">&times;</span></button>
        </div>
      </div>
  </form>
</div>

{{> displayTable}}